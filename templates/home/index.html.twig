{% extends 'base.html.twig' %}



{% block body %}
<!DOCTYPE html>
<html class="no-js" lang="zxx">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{% block title %}Ooredoo- Home{% endblock %}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="manifest" href="{{asset('site.webmanifest')}}">
    <link rel="shortcut icon" type="image/x-icon" href="{{asset('front-office/assets/img/favicon.ico')}}">

    <!-- CSS here -->
    <link rel="stylesheet" href="{{asset('front-office/assets/css/bootstrap.min.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/owl.carousel.min.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/slicknav.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/flaticon.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/gijgo.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/animate.min.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/animated-headline.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/magnific-popup.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/fontawesome-all.min.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/themify-icons.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/slick.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/nice-select.css')}}">
    <link rel="stylesheet" href="{{asset('front-office/assets/css/style.css')}}">
</head>
<body class="black-bg">
{% block WholeBody %}

    <header>
        <!-- Header Start -->
        <div class="header-area header-transparent">
            <div class="main-header header-sticky">
                <div class="container-fluid">
                    <div class="menu-wrapper d-flex align-items-center justify-content-between">
                        <!-- Logo -->
                        <div class="logo">
                            <a href="{{path('home')}}"><img src="{{ asset('front-office/assets/img/logo/logo.png') }}" alt="Symfony!"style="max-width:40%;
height: auto;  "></a>
                        </div>
                        <!-- Main-menu -->
                        <div class="main-menu f-right d-none d-lg-block">
                            <nav>
                                <ul id="navigation">
                                    <li><a href="{{path('home')}}">Home</a></li>
                                    <li><a href="{{asset('reclamationpath')}}">Contact Us</a></li>
                                </ul>
                            </nav>
                        </div>
                        <!-- Header-btn -->
                        <div class="header-btns d-none d-lg-block f-right">
                            <a href="{{path('app_login')}}" class="btn">Sign in</a>
                        </div>
                        <!-- Mobile Menu -->
                        <div class="col-12">
                            <div class="mobile_menu d-block d-lg-none"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Header End -->
    </header>
    <main>
        <!--? slider Area Start-->
        <div class="slider-area position-relative">
            <div class="slider-active">
                <!-- Single Slider -->
                <div class="single-slider slider-height d-flex align-items-center">
                    <div class="container">
                        <div class="row">
                            <div class="col-xl-9 col-lg-9 col-md-10">
                                <div class="hero__caption">
                                    <span data-animation="fadeInLeft" data-delay="0.1s">Core Infra</span>
                                    <h1 data-animation="fadeInLeft" data-delay="0.4s" style=" color:#ed1c24">Management Tools</h1>
                                    <a href="{{path('app_register')}}" class="border-btn hero-btn" data-animation="fadeInLeft" data-delay="0.8s">Sign up</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- slider Area End-->


    <!-- Scroll Up -->
    <div id="back-top" >
        <a title="Go to Top" href="#"> <i class="fas fa-level-up-alt"></i></a>
    </div>

    <!-- JS here -->

    <script src="{{ asset('./front-office/assets/js/vendor/modernizr-3.5.0.min.js')}}"></script>
    <!-- Jquery, Popper, Bootstrap -->
    <script src="{{ asset('./front-office./assets/js/vendor/jquery-1.12.4.min.js')}}"></script>
    <script src="{{ asset('./front-office./assets/js/popper.min.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/bootstrap.min.js')}}"></script>
    <!-- Jquery Mobile Menu -->
    <script src="{{ asset('./front-office/assets/js/jquery.slicknav.min.js')}}"></script>

    <!-- Jquery Slick , Owl-Carousel Plugins -->
    <script src="{{ asset('./front-office/assets/js/owl.carousel.min.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/slick.min.js')}}"></script>
    <!-- One Page, Animated-HeadLin -->
    <script src="{{ asset('./front-office/assets/js/wow.min.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/animated.headline.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/jquery.magnific-popup.js')}}"></script>

    <!-- Date Picker -->
    <script src="{{ asset('./front-office/assets/js/gijgo.min.js')}}"></script>
    <!-- Nice-select, sticky -->
    <script src="{{ asset('./front-office/assets/js/jquery.nice-select.min.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/jquery.sticky.js')}}"></script>

    <!-- counter , waypoint,Hover Direction -->
    <script src="{{ asset('./front-office/assets/js/jquery.counterup.min.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/waypoints.min.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/jquery.countdown.min.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/hover-direction-snake.min.js')}}"></script>

    <!-- contact js -->
    <script src="{{ asset('./front-office/assets/assets/js/contact.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/jquery.form.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/jquery.validate.min.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/mail-script.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/jquery.ajaxchimp.min.js')}}"></script>

    <!-- Jquery Plugins, main Jquery -->
    <script src="{{ asset('./front-office/assets/js/plugins.js')}}"></script>
    <script src="{{ asset('./front-office/assets/js/main.js')}}"></script>

{% endblock %}
</main>
</body>
</html>

{% endblock %}
